	"version": "0.2.0",
	"configurations": [
		{
			"name": "Compile",
			"type": "pwa-node",
			"request": "launch",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}\\src\\ethereum\\compile.js"
		},
		{
			"name": "Mocha Tests",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceFolder}\\src\\ethereum",
			"runtimeExecutable": "mocha"
		},
		{
			"name": "Deploy",
			"program": "${workspaceFolder}\\src\\ethereum\\deploy.js",
			"request": "launch",
			"skipFiles": ["<node_internals>/**"],
			"type": "pwa-node"
		}
	]
}
